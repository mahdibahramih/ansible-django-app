server{
    listen 80 ;
  ##  server_name {{ domain }}
    location /media  {
        alias /srv/{{ project_name }}/media;  # your Django project's media files - amend as required
    }

    location /static {
        alias /srv/{{ project_name }}/web/static; # your Django project's static files - amend as required
    }
    location / {    
         include  uwsgi_params;
        uwsgi_pass 127.0.0.1:3031;

    }
}